<script type="text/javascript">
    {% for riot_tag in riot_tags %}
        riot.compile("{{riot_tag}}")
    {% endfor %}
    $(document).ready(function(){
        d_visual_designer.initLocal({{local|json_encode(constant('JSON_FORCE_OBJECT'))}})
        d_visual_designer.initOptions({{options|json_encode(constant('JSON_FORCE_OBJECT'))}})
        d_visual_designer.initState({{state|json_encode(constant('JSON_FORCE_OBJECT'))}})
        d_visual_designer.initContent();
    })
</script>